        [ValveVp02] = PneumaticValveTags.Vp0_2_GasG25_SbOpen,
        [ValveVp03] = PneumaticValveTags.Vp0_3_LpgG30_SbOpen,
        [ValveEv03] = PneumaticValveTags.Ev0_3_Gas_SbOpen
    };

    // Значения датчиков
    private double _vpp3_1Value;
    private double _gasPaValue;
    private double _gasTagValue;
    private double _gasPogValue;
    private double _gasPValue;
    private double _gasPagValue;
    private double _gasPgbValue;
    private bool _disposed;

    /// <summary>
    /// Подписывается на OPC-UA теги клапанов при инициализации.
    /// </summary>
    protected override async Task OnInitializedAsync()
    {
        // Клапаны (подписка на xOut_Open для индикации)
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp0_1_GasG20, OnVp01Changed, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp0_2_GasG25, OnVp02Changed, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp0_3_LpgG30, OnVp03Changed, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(PneumaticValveTags.Ev0_3_Gas, OnEv03Changed, emitCachedValueImmediately: EmitCachedValueImmediately);

        // Датчики
        await Subscription.SubscribeAsync("ns=3;s=\"DB_ControlValve\".\"VPP3_1\".\"ActuelValue\"", OnVpp31Changed, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(SensorScreenTags.GasPa, OnGasPaChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(MainScreenTags.GasTag, OnGasTagChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(SensorScreenTags.GasPog, OnGasPogChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(SensorScreenTags.GasP, OnGasPChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(SensorScreenTags.GasPag, OnGasPagChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
        await Subscription.SubscribeAsync(MainScreenTags.GasPgb, OnGasPgbChanged, emitCachedValueImmediately: EmitCachedValueImmediately);
    }

    // Callbacks для клапанов (методы вместо лямбд для корректной отписки)
    private Task OnVp01Changed(object? v) => OnValveChangedAsync("VP0_1", v);
    private Task OnVp02Changed(object? v) => OnValveChangedAsync("VP0_2", v);
    private Task OnVp03Changed(object? v) => OnValveChangedAsync("VP0_3", v);
    private Task OnEv03Changed(object? v) => OnValveChangedAsync("EV0_3", v);

    /// <summary>
    /// Обрабатывает изменение состояния клапана.
    /// </summary>
    private async Task OnValveChangedAsync(string valveId, object? value)
    {
        if (_disposed)
        {
            return;
        }

        _valveStates[valveId] = value is true;
        await InvokeAsync(StateHasChanged);
    }

    /// <summary>
    /// Переключает состояние клапана (инвертирует xSB_Open).
    /// </summary>
    private async Task ToggleValveAsync(string valveId)
    {
        if (_disposed)
        {
            return;
        }

        if (!ValveSbOpenTags.TryGetValue(valveId, out var nodeId))
        {
            return;
        }

        var result = await TagService.ReadAsync<bool>(nodeId);
        if (result.Success)
        {
            await TagService.WriteAsync(nodeId, !result.Value);
        }
    }

    private async Task OnVpp31Changed(object? v) { if (_disposed) return; _vpp3_1Value = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasPaChanged(object? v) { if (_disposed) return; _gasPaValue = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasTagChanged(object? v) { if (_disposed) return; _gasTagValue = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasPogChanged(object? v) { if (_disposed) return; _gasPogValue = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasPChanged(object? v) { if (_disposed) return; _gasPValue = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasPagChanged(object? v) { if (_disposed) return; _gasPagValue = ToDouble(v); await InvokeAsync(StateHasChanged); }
    private async Task OnGasPgbChanged(object? v) { if (_disposed) return; _gasPgbValue = ToDouble(v); await InvokeAsync(StateHasChanged); }

    private static double ToDouble(object? value) => value switch
    {
        double d => d,
        float f => f,
        int i => i,
        _ => 0
    };

    /// <summary>
    /// Получает цвет заливки для клапана.
    /// </summary>
    private string GetValveColor(string valveId) =>
        _valveStates.TryGetValue(valveId, out var isOpen) && isOpen ? OpenColor : ClosedColor;

    /// <summary>
    /// Получает тёмный цвет заливки для клапана (для полосы).
    /// </summary>
    private string GetValveColorDark(string valveId) =>
        _valveStates.TryGetValue(valveId, out var isOpen) && isOpen ? OpenColorDark : ClosedColorDark;

    /// <summary>
    /// Возвращает URL градиента для трубы в зависимости от состояния клапана.
    /// Открыт = жёлтый (-open), закрыт = серый (оригинал).
    /// </summary>
    private string GetPipeGradient(string valveId, string gradientId)
    {
        var isOpen = _valveStates.TryGetValue(valveId, out var state) && state;
        var suffix = isOpen ? "-open" : "";
        return $"url(#{gradientId}{suffix})";
    }

    /// <summary>
    /// Проверяет, открыт ли хотя бы один из указанных клапанов.
    /// </summary>
    private bool IsAnyValveOpen(params string[] valveIds) =>
        valveIds.Any(id => _valveStates.TryGetValue(id, out var state) && state);

    /// <summary>
    /// Возвращает URL градиента для трубы, зависящей от нескольких клапанов.
    /// Если любой из указанных клапанов открыт - жёлтый (-open), иначе серый.
    /// </summary>
    private string GetPipeGradientForAny(string[] valveIds, string gradientId)
    {
        var suffix = IsAnyValveOpen(valveIds) ? "-open" : "";
        return $"url(#{gradientId}{suffix})";
    }

    /// <summary>
    /// Возвращает градиент для трубы после EV0.3.
    /// Жёлтый (-open) если EV0.3 открыт И хотя бы один VP открыт.
    /// </summary>
    private string GetPipeGradientAfterEv03(string gradientId)
    {
        var isActive = IsAnyValveOpen("EV0_3") && IsAnyValveOpen("VP0_1", "VP0_2", "VP0_3");
        var suffix = isActive ? "-open" : "";
        return $"url(#{gradientId}{suffix})";
    }

    /// <summary>
    /// Отписывается от OPC-UA тегов при уничтожении компонента.
    /// </summary>
    public async ValueTask DisposeAsync()
    {
        _disposed = true;

        // Клапаны (отписка от xOut_Open)
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp0_1_GasG20, OnVp01Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp0_2_GasG25, OnVp02Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp0_3_LpgG30, OnVp03Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Ev0_3_Gas, OnEv03Changed);

        // Датчики
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_ControlValve\".\"VPP3_1\".\"ActuelValue\"", OnVpp31Changed);
        await Subscription.UnsubscribeAsync(SensorScreenTags.GasPa, OnGasPaChanged);
        await Subscription.UnsubscribeAsync(MainScreenTags.GasTag, OnGasTagChanged);
        await Subscription.UnsubscribeAsync(SensorScreenTags.GasPog, OnGasPogChanged);
        await Subscription.UnsubscribeAsync(SensorScreenTags.GasP, OnGasPChanged);
        await Subscription.UnsubscribeAsync(SensorScreenTags.GasPag, OnGasPagChanged);
        await Subscription.UnsubscribeAsync(MainScreenTags.GasPgb, OnGasPgbChanged);
    }
}
