    /// <inheritdoc />
    protected override async Task OnInitializedAsync()
    {
        // Подписка на xOut_Open для индикации
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_1_Purge, OnVp2_1Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_2_Purge, OnVp2_2Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_3_HighPressure, OnVp2_3Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_4_Fill, OnVp2_4Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_5_BoilerOut, OnVp2_5Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_6_Drain, OnVp2_6Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_7_BoilerIn, OnVp2_7Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp2_8_Drain, OnVp2_8Changed);

        // Датчики
        await Subscription.SubscribeAsync("ns=3;s=\"DB_Measure\".\"Temper\".\"DHW_TUS\"", OnDhwTusChanged);
        await Subscription.SubscribeAsync("ns=3;s=\"DB_Measure\".\"Sensor\".\"DHW_PES\"", OnDhwPesChanged);
        await Subscription.SubscribeAsync("ns=3;s=\"DB_Measure\".\"Temper\".\"DHW_TES\"", OnDhwTesChanged);
        await Subscription.SubscribeAsync("ns=3;s=\"DB_Measure\".\"Sensor\".\"DHW_FS\"", OnDhwFsChanged);
        await Subscription.SubscribeAsync("ns=3;s=\"DB_ControlValve\".\"VPP3_1\".\"ActuelValue\"", OnVpp3_1Changed);
    }

    private Task OnVp2_1Changed(object? v) => OnValveChangedAsync("VP2_1", v);
    private Task OnVp2_2Changed(object? v) => OnValveChangedAsync("VP2_2", v);
    private Task OnVp2_3Changed(object? v) => OnValveChangedAsync("VP2_3", v);
    private Task OnVp2_4Changed(object? v) => OnValveChangedAsync("VP2_4", v);
    private Task OnVp2_5Changed(object? v) => OnValveChangedAsync("VP2_5", v);
    private Task OnVp2_6Changed(object? v) => OnValveChangedAsync("VP2_6", v);
    private Task OnVp2_7Changed(object? v) => OnValveChangedAsync("VP2_7", v);
    private Task OnVp2_8Changed(object? v) => OnValveChangedAsync("VP2_8", v);

    /// <summary>
    /// Обрабатывает изменение состояния клапана.
    /// </summary>
    private async Task OnValveChangedAsync(string valveId, object? value)
    {
        if (_disposed) return;
        _valveStates[valveId] = value is true;
        await InvokeAsync(StateHasChanged);
    }

    /// <summary>
    /// Переключает состояние клапана (инвертирует xSB_Open).
    /// </summary>
    private async Task ToggleValveAsync(string valveId)
    {
        if (_disposed)
        {
            return;
        }

        if (!ValveSbOpenTags.TryGetValue(valveId, out var nodeId))
        {
            return;
        }

        var result = await TagService.ReadAsync<bool>(nodeId);
        if (result.Success)
        {
            await TagService.WriteAsync(nodeId, !result.Value);
        }
    }

    private async Task OnDhwTusChanged(object? v)
    {
        if (_disposed) return;
        _dhwTusValue = ToDouble(v);
        await InvokeAsync(StateHasChanged);
    }

    private async Task OnDhwPesChanged(object? v)
    {
