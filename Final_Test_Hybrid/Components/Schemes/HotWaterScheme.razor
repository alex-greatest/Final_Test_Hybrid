    /// <inheritdoc />
    public async ValueTask DisposeAsync()
    {
        _disposed = true;
        // Отписка от xOut_Open
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_1_Purge, OnVp2_1Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_2_Purge, OnVp2_2Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_3_HighPressure, OnVp2_3Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_4_Fill, OnVp2_4Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_5_BoilerOut, OnVp2_5Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_6_Drain, OnVp2_6Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_7_BoilerIn, OnVp2_7Changed);
        await Subscription.UnsubscribeAsync(PneumaticValveTags.Vp2_8_Drain, OnVp2_8Changed);

        // Датчики
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_Measure\".\"Temper\".\"DHW_TUS\"", OnDhwTusChanged);
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_Measure\".\"Sensor\".\"DHW_PES\"", OnDhwPesChanged);
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_Measure\".\"Temper\".\"DHW_TES\"", OnDhwTesChanged);
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_Measure\".\"Sensor\".\"DHW_FS\"", OnDhwFsChanged);
        await Subscription.UnsubscribeAsync("ns=3;s=\"DB_ControlValve\".\"VPP3_1\".\"ActuelValue\"", OnVpp3_1Changed);
    }
