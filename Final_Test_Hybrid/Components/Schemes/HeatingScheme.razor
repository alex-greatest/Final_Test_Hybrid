    /// </summary>
    protected override async Task OnInitializedAsync()
    {
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_1_Purge, OnVp1_1Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_2_Purge, OnVp1_2Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_3_Fill, OnVp1_3Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_4_FastFill, OnVp1_4Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_5_WaterToCh, OnVp1_5Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_6_WaterToCh, OnVp1_6Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_7_HotWaterNoPump, OnVp1_7Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_8_Drain, OnVp1_8Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_9_Reserve, OnVp1_9Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_10_HotWaterWithPump, OnVp1_10Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_11_Drain, OnVp1_11Changed);
        await Subscription.SubscribeAsync(PneumaticValveTags.Vp1_12_SlowFill, OnVp1_12Changed);
    }

