    /// <summary>
    /// Обрабатывает изменение состояния клапана.
    /// </summary>
    private async Task OnValveChangedAsync(string valveId, object? value)
    {
        if (_disposed)
        {
            return;
        }

        _valveStates[valveId] = value is true;
        await InvokeAsync(StateHasChanged);
    }

    /// <summary>
    /// Получает цвет заливки для клапана.
    /// </summary>
    private string GetValveColor(string valveId) =>
        _valveStates.TryGetValue(valveId, out var isOpen) && isOpen ? OpenColor : ClosedColor;

    /// <summary>
    /// Получает тёмный цвет заливки для клапана (для полосы).
    /// </summary>
    private string GetValveColorDark(string valveId) =>
        _valveStates.TryGetValue(valveId, out var isOpen) && isOpen ? OpenColorDark : ClosedColorDark;

